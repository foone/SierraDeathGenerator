	"null-character": 32,
	"first-height": 24,
	"hooks": {
		"pre-parse": "fontInfo['first-height']=fontInfo['overlays']['scene']['options'][$('#overlay-scene option:selected').text()]['info']['first-height']",
		"font": "{let scene = overlays.scene.data.info;originx=scene.x;mainFont.y=scene.y}",
		"border": "if(overlays.scene.name=='failure'){bw=208;bh=80;border_x=24;border_y=135}else{let firstLine = rawtext.match(/^(.*)\\n/);if(firstLine){bw=16+firstLine[1].trimStart().length*8;bh=32;border_x=originx-8+8*(rawtext.match(/^( *)/)[1].length);border_y=mainFont.y-8;};if(overlays.scene.name=='secret_command'){border_sides='ffffffftf'}}",
		"change-scene": "{let txt = $('#sourcetext').val(); if(isAnyDefaultText(txt) || Object.values(fontInfo.overlays.scene.options).map(o=>o.info.text).includes(txt)){$('#sourcetext').val(overlays.scene.options[$(this).val()].info.text)}}"
	},


	"overlays": {
		"scene": {
			"x": 0,
			"y": 0,
